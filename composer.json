{
    "name": "laravel-nodenotifierdesktop/laravel-nodenotifierdesktop",
    "description": "Laravel package for desktop notifications using node-notifier",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "idpcks",
            "email": "mail.idpcks@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelNodeNotifierDesktop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelNodeNotifierDesktop\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelNodeNotifierDesktop\\LaravelNodeNotifierDesktopServiceProvider"
            ],
            "aliases": {
                "DesktopNotifier": "LaravelNodeNotifierDesktop\\Facades\\DesktopNotifier"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 